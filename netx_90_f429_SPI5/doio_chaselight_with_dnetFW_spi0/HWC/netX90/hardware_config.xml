<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<hwconfig tool_version="4.1.9" peripherals_version="1.4.0.12" chip_type="netx90_rev1" board="default">
    <peripherals>
        <peripheral id="general">
            <config id="default">
                <parameter id="file_version" value="0"/>
                <parameter id="app_cpu_enable" value="true"/>
                <parameter id="struct_version" value="2"/>
                <parameter id="firewall_sqi_flash" value="com_only"/>
                <parameter id="firewall_pad_ctrl" value="no_access"/>
                <parameter id="firewall_eth_mac" value="app_only"/>
                <parameter id="firewall_madc" value="app_only"/>
                <parameter id="firewall_sdram" value="com_and_app"/>
                <parameter id="firewall_uart" value="com_only"/>
                <parameter id="firewall_debug_slave" value="no_access"/>
                <parameter id="idpm_enable" value="true"/>
                <parameter id="file_text" value=""/>
                <parameter id="firewall_crypt_system" value="com_only"/>
                <parameter id="firewall_extmem" value="app_only"/>
            </config>
        </peripheral>
        <peripheral id="mmio4"/>
        <peripheral id="mmio5"/>
        <peripheral id="mmio6"/>
        <peripheral id="mmio7"/>
        <peripheral id="spi0_app">
            <config id="default">
                <parameter id="spi0_app_cs1n" value="enabled"/>
            </config>
        </peripheral>
        <peripheral id="spm0">
            <config id="spm_spi">
                <parameter id="mode" value="3"/>
                <parameter id="sdpm_miso_early" value="0"/>
                <parameter id="dpm0_spi_dirq" value="enabled"/>
                <parameter id="dpm0_spi_sirq" value="disabled"/>
            </config>
        </peripheral>
        <peripheral id="uart">
            <config id="uart"/>
        </peripheral>
        <peripheral id="xm0">
            <config id="xm0_custom">
                <parameter id="reset_out_usage" value="0"/>
                <parameter id="hw_option" value="0x0000"/>
                <parameter id="xm0_io0" value="disabled"/>
            </config>
        </peripheral>
    </peripherals>
    <io_config>
        <pin id="HIF_A11" peripheral="spi0_app" function="spi0_app_clk"/>
        <pin id="HIF_A12" peripheral="spi0_app" function="spi0_app_cs1n"/>
        <pin id="HIF_A13" peripheral="spi0_app" function="spi0_app_cs0n"/>
        <pin id="HIF_A14" peripheral="spi0_app" function="spi0_app_mosi"/>
        <pin id="HIF_A15" peripheral="spi0_app" function="spi0_app_miso"/>
        <pin id="HIF_D10" peripheral="spm0" function="dpm0_spi_csn"/>
        <pin id="HIF_D11" peripheral="spm0" function="dpm0_spi_clk"/>
        <pin id="HIF_D12" peripheral="spm0" function="dpm0_spi_dirq"/>
        <pin id="HIF_D8" peripheral="spm0" function="dpm0_spi_miso"/>
        <pin id="HIF_D9" peripheral="spm0" function="dpm0_spi_mosi"/>
        <pin id="MII0_RXD0" peripheral="xm0" function="xm0_rx"/>
        <pin id="MII0_RXD1" peripheral="xm0" function="xm0_tx"/>
        <pin id="MLED0" peripheral="xm0" function="mled0"/>
        <pin id="MLED1" peripheral="xm0" function="mled1"/>
        <pin id="MMIO4" peripheral="mmio4" function="mmio4"/>
        <pin id="MMIO5" peripheral="mmio5" function="mmio5"/>
        <pin id="MMIO6" peripheral="mmio6" function="mmio6"/>
        <pin id="MMIO7" peripheral="mmio7" function="mmio7"/>
        <pin id="UART_RXD" peripheral="uart" function="uart_rxd"/>
        <pin id="UART_TXD" peripheral="uart" function="uart_txd"/>
    </io_config>
    <mmio_config/>
    <pad_config>
        <pin id="HIF_A11" drive_strength="low" pull_enable="true"/>
        <pin id="HIF_A12" drive_strength="low" pull_enable="true"/>
        <pin id="HIF_A13" drive_strength="low" pull_enable="true"/>
        <pin id="HIF_A14" drive_strength="low" pull_enable="true"/>
        <pin id="HIF_A15" drive_strength="low" pull_enable="true"/>
        <pin id="HIF_D10" drive_strength="low" pull_enable="true"/>
        <pin id="HIF_D11" drive_strength="low" pull_enable="true"/>
        <pin id="HIF_D12" drive_strength="low" pull_enable="true"/>
        <pin id="HIF_D8" drive_strength="high" pull_enable="true"/>
        <pin id="HIF_D9" drive_strength="low" pull_enable="true"/>
        <pin id="MII0_RXD0" pull_enable="true"/>
        <pin id="MII0_RXD1" drive_strength="low" pull_enable="true"/>
        <pin id="MLED0" drive_strength="high" pull_enable="false"/>
        <pin id="MLED1" drive_strength="high" pull_enable="false"/>
        <pin id="MMIO4" drive_strength="low" pull_enable="true"/>
        <pin id="MMIO5" drive_strength="low" pull_enable="true"/>
        <pin id="MMIO6" drive_strength="low" pull_enable="true"/>
        <pin id="MMIO7" drive_strength="low" pull_enable="true"/>
        <pin id="UART_RXD" pull_enable="true"/>
        <pin id="UART_TXD" drive_strength="low" pull_enable="true"/>
    </pad_config>
</hwconfig>
